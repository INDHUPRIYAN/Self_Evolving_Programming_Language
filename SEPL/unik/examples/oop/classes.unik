class Animal {
    func speak() { give "..." }
}

trait Walker {
    func walk() { give "Walking..." }
}

class Dog : Animal, Walker {
    init(name) { self.name = name }
    func speak() { give "Woof! I am ", self.name }
}

d = Dog("Bruno")
d.speak()  # Woof! I am Bruno
d.walk()   # Walking...
